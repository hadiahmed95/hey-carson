+
+  // Clear previous timeout
+  if (userSearchTimeout.value) {
+    clearTimeout(userSearchTimeout.value);
+  }
+
+  // If user clears the input or it's too short, clear results immediately
+  if (!searchTerm || searchTerm.length < 2) {
+    users.value = [];
